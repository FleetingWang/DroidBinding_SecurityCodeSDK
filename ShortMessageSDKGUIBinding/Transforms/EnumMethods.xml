<enum-method-mappings>
  <!--
  This example changes the Java method:
    android.support.v4.app.Fragment.SavedState.writeToParcel (int flags)
  to be:
    android.support.v4.app.Fragment.SavedState.writeToParcel (Android.OS.ParcelableWriteFlags flags)
  when bound in C#.
  
  <mapping jni-class="android/support/v4/app/Fragment.SavedState">
    <method jni-name="writeToParcel" parameter="flags" clr-enum-type="Android.OS.ParcelableWriteFlags" />
  </mapping>
  -->
  
  <mapping jni-class="cn/smssdk/gui/ContactEntry">
    <method jni-name="getKey" parameter="return" clr-enum-type="Java.Lang.Object" />
  </mapping>
  <mapping jni-class="cn/smssdk/gui/ContactEntry">
    <method jni-name="getValue" parameter="return" clr-enum-type="Java.Lang.Object" />
  </mapping>
  <mapping jni-class="cn/smssdk/gui/ContactEntry">
    <method jni-name="setValue" parameter="return" clr-enum-type="Java.Lang.Object" />
  </mapping>
  <mapping jni-class="cn/smssdk/gui/ContactsAdapter">
    <method jni-name="getItem" parameter="return" clr-enum-type="Java.Lang.Object" />
  </mapping>
  <mapping jni-class="cn/smssdk/gui/CountryAdapter">
    <method jni-name="getItem" parameter="return" clr-enum-type="Java.Lang.Object" />
  </mapping>
</enum-method-mappings>